      <xs:element name="gameLobbyName" type="xs:string" minOccurs="0"/>
      <xs:element name="gamePlayerList" type="tns:playerIdentification" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="passwordProtected" type="xs:boolean"/>
      <xs:element name="playerLimit" type="xs:int"/>
    </xs:sequence>
  </xs:complexType>

