<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" targetNamespace="http://webservices.org/" xmlns:tns="http://webservices.org/" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="Exception" type="tns:Exception"/>

  <xs:element name="createGameLobby" type="tns:createGameLobby"/>

  <xs:element name="createGameLobbyResponse" type="tns:createGameLobbyResponse"/>

  <xs:element name="getGameLobbies" type="tns:getGameLobbies"/>

  <xs:element name="getGameLobbiesResponse" type="tns:getGameLobbiesResponse"/>

  <xs:element name="getGameLobby" type="tns:getGameLobby"/>

  <xs:element name="getGameLobbyResponse" type="tns:getGameLobbyResponse"/>

  <xs:element name="getPlayerByID" type="tns:getPlayerByID"/>

  <xs:element name="getPlayerByIDResponse" type="tns:getPlayerByIDResponse"/>

  <xs:element name="joinGameLobby" type="tns:joinGameLobby"/>

  <xs:element name="joinGameLobbyResponse" type="tns:joinGameLobbyResponse"/>

  <xs:element name="leaveGameLobby" type="tns:leaveGameLobby"/>

  <xs:element name="leaveGameLobbyResponse" type="tns:leaveGameLobbyResponse"/>

  <xs:element name="login" type="tns:login"/>

  <xs:element name="loginResponse" type="tns:loginResponse"/>

  <xs:element name="logout" type="tns:logout"/>

  <xs:element name="logoutResponse" type="tns:logoutResponse"/>

  <xs:complexType name="login">
    <xs:sequence>
      <xs:element name="username" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="loginResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:player" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="player">
    <xs:complexContent>
      <xs:extension base="tns:playerIdentification">
        <xs:sequence>
          <xs:element name="playerID" type="xs:string" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="playerIdentification">
    <xs:sequence>
      <xs:element name="username" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getPlayerByID">
    <xs:sequence>
      <xs:element name="playerID" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getPlayerByIDResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:player" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="leaveGameLobby">
    <xs:sequence>
      <xs:element name="playerID" type="xs:string" minOccurs="0"/>
      <xs:element name="GameLobbyID" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="leaveGameLobbyResponse">
    <xs:sequence/>
  </xs:complexType>

  <xs:complexType name="joinGameLobby">
    <xs:sequence>
      <xs:element name="playerID" type="xs:string" minOccurs="0"/>
      <xs:element name="GameLobbyID" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="joinGameLobbyResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:gameLobby" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="gameLobby">
    <xs:sequence>
      <xs:element name="gameLobbyAdmin" type="xs:string" minOccurs="0"/>
      <xs:element name="gameLobbyID" type="xs:string" minOccurs="0"/>
      <xs:element name="gameLobbyName" type="xs:string" minOccurs="0"/>
      <xs:element name="gamePlayerListPublic" type="xs:string" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Exception">
    <xs:sequence>
      <xs:element name="message" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getGameLobby">
    <xs:sequence>
      <xs:element name="GameLobbyID" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getGameLobbyResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:gameLobby" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="createGameLobby">
    <xs:sequence>
      <xs:element name="playerID" type="xs:string" minOccurs="0"/>
      <xs:element name="gameLobbyName" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="createGameLobbyResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:gameLobby" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getGameLobbies">
    <xs:sequence/>
  </xs:complexType>

  <xs:complexType name="getGameLobbiesResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:gameLobby" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="logout">
    <xs:sequence>
      <xs:element name="playerID" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="logoutResponse">
    <xs:sequence>
      <xs:element name="return" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>

